generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Debt {
  id          Int      @id @default(autoincrement())
  userId      Int
  amount      Int
  description String
  paid        Boolean  @default(false)
  createdAt   DateTime @default(now())
}
